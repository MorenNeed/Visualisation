<Window x:Class="Visualisation.InputData"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Visualisation"
        mc:Ignorable="d"
        Title="InputData" Height="200" Width="300">
    <Grid>
        <TextBlock Text="Input Data" VerticalAlignment="Top" HorizontalAlignment="Center" Margin="10"/>
        <TextBox x:Name="Data_TextBox" HorizontalAlignment="Center" TextWrapping="Wrap" Text="DATA" TextAlignment="Center" VerticalAlignment="Center" Width="120" FontWeight="Normal"/>
        <Button Content="Execute!" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="10" BorderBrush="White" Background="White" Click="Button_Click"/>
    </Grid>
</Window>
